{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "isolatedModules": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@icons/*": [
        "src/icons/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ]
    },
  },
}